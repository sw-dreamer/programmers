import java.util.ArrayList;
class Solution {
	public ArrayList<Long> solution(int x, int n) {
		ArrayList<Long> answer = new ArrayList<>();
		long temp = 0;
		if ((-10000000 <= x && x <= 10000000) && (0 <= n && n <= 1000)) {
			for (int i = 0; i < n; i++) {
				temp += x;
				answer.add(temp);
			}
		}
		return answer;
	}
}
