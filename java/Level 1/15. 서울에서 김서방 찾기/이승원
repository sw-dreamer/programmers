import java.util.HashMap;


class Solution {
   public String solution(String[] seoul) {
       String answer = "";
       HashMap<String, Integer> answerMap = new HashMap<>();
       for(int i = 0 ; i<seoul.length ; i++) {
       	answerMap.put(seoul[i], i);
       }
      
       answer = "김서방은 " + answerMap.get("Kim") + "에 있다";
       return answer;
   }
}
