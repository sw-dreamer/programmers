import java.util.Scanner;

class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        String num = "*";
        
        for(int i = 0 ; i < b ; i++ ){
            System.out.print(num.repeat(a)+"\n"); 
        }

    }
}
