class Solution {
	    public int solution(String t, String p) {
	        int count = 0;
	        int pLength = p.length();
	        long pValue = Long.parseLong(p);
	        
	        for(int i=0;i<=t.length()-pLength;i++){
	        	long subValue = 0;
	        	for(int j=0;j<pLength;j++) {
	        		subValue = subValue * 10 + (t.charAt(i+j)-'0');
	        	}
	            if(subValue <= pValue) {
	            	count ++;
	            }
	        }
	        return count;
	    }
	}
