import java.util.*;

public class Solution {
    public int solution(int n) {

		int sum = 0;
		int C = 0;
		int answer = 0;
		for (int i = 8; i >= 0; i--) {
			int A = n /(int)(Math.pow(10, i));  
			int B = n/(int)(Math.pow(10, i+1)); 
			C = A -B*(10);
			answer = answer + C;

		} return answer;
    }
}
