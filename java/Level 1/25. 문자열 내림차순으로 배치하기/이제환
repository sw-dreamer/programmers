import java.util.Arrays;
import java.util.Collections;

class Solution {
    public String solution(String s) {
        String answer = "";
        String [] strArray = s.split("");
        
        Arrays.sort(strArray, Collections.reverseOrder());
        
        for(int i = 0; i < strArray.length; i++){
            answer += strArray[i];
        }
        return answer;
    }
}
